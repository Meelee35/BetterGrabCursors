{
	"manifest_version": 2,
	"name": "Grab Cursors",
	"description": "Better Grab Cursors",
	"version": "1.5",
	"permissions": [
			"storage"
	],
	"background": {
			"scripts": ["background.js"],
			"persistent": false
	},
	"content_scripts": [
			{
					"matches": ["<all_urls>"],
					"js": ["content.js"]
			}
	],
	"icons": {
		"48": "icons/icon.svg",
		"96": "icons/icon.svg",
		"32": "icons/icon.svg",
		"16": "icons/icon.svg",
		"128": "icons/icon.svg"

	}
}