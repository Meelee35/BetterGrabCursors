{
  "manifest_version": 3,
  "name": "Grab Cursors",
  "description": "Better Grab Cursors",
  "version": "1.7",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "cursors/grab1.cur",
        "cursors/grabbing1.cur"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
